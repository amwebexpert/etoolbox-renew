{
  "name": "my-vue-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "clean:node": "npx rimraf node_modules yarn.lock",
    "start": "yarn dev",
    "dev": "vite",
    "typecheck": "npx tsc --noEmit --project tsconfig.app.json",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=4096 rimraf dist && yarn generate:version && tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "generate:version": "node scripts/generate-version",
    "generate:api:client": "npx @hey-api/openapi-ts -i ./public/openapi/openapi.json -o ./src/api/generated --client @hey-api/client-axios",
    "deploy": "bash scripts/deploy.sh",
    "test": "vitest",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "npm:rolldown-vite@7.2.5"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.5"
  }
}
